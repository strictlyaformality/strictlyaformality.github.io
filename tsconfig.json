{
  "compilerOptions": {
    // "incremental": true,                   /* Enable incremental compilation */
    "target": "es5", 
    "module": "commonjs", 
    "jsx": "preserve", 
    "strict": true, 
    "baseUrl": ".", 
    "paths": {
      "components/*": [
        "components/*"
      ],
      "pages/*": [
        "pages/*"
      ],
      "images/*": [
        "images/*"
      ],
      "styles/*": [
        "styles/*"
      ],
      "hooks/*": [
        "hooks/*"
      ]
    },
    "esModuleInterop": true, 
    "skipLibCheck": true, 
    "forceConsistentCasingInFileNames": true,
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "noEmit": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
